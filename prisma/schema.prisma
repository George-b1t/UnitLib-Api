generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id       Int     @id @unique @default(autoincrement())
  name     String  @unique
  password String
  isAdm    Boolean

  @@map("users")
}

model Book {
  id           Int     @id @unique @default(autoincrement())
  name         String
  author       String
  genre        String
  pdf_location String?

  @@map("book")
}
